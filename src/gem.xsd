<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="gem_xsd"
	xmlns="gem_xsd" 
	xmlns:jewel="jewel_xsd"
	version="0.1" 
	elementFormDefault="qualified">
	
	<xs:import namespace="jewel_xsd"
               schemaLocation="jewel.xsd" />
               
	<xs:element name="Gem">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="jewel" type="jewel:JewelType" 
					maxOccurs="99" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
</xs:schema>